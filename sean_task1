arr=[23,34,25,567,42,87,4,8,65]
ln=len(arr)
K=int(input("Enter Value for K: "))
br=0
for i in range(ln-2):
    if br==1: break
    if arr[i]>=K: continue
    for j in range(ln-1):
        if br==1: break
        if arr[i]+arr[j]>=K: continue
        for k in range(ln):
            if arr[i]+arr[j]+arr[k]==K: 
                print("True",arr[i],arr[j],arr[k])
                br=1
                break
if br==0: print("False")
